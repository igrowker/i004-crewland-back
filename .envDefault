DB_USERNAME=               # Nombre de usuario de PostgreSQL (por defecto es postgre)
DB_PASSWORD=               # Constraseña cuando hayan creado su DB en PostgreSQL
DB_NAME=                   # Nombre de la DB (crewland en nuestro caso)

